<header class="mb-4">
  <nav class="flex flex-wrap items-center justify-between px-10 py-3 text-gray-700 bg-black">

    <div class="flex items-center mr-6 flex-no-shrink">
      <%= link_to "/", class: "logo w-10" do %>
        <%= cl_image_tag "https://res.cloudinary.com/dyeuxstgg/image/upload/v1626885908/KombiCode/logo_192_gokswj.svg" %>
      <% end %>
    </div>

    <div class="flex flex-row items-center justify-end mt-0 mr-10 text-right">
      <%= turbo_frame_tag 'contact_modal' %>
      <div class="flex-grow" data-controller="extended-modal"
           data-extended-modal-prevent-default-action-opening="false"
           data-extended-modal-from-value="11" >
        <%= render 'contacts/modal'%>
        <i class="fas fa-arrow-right"></i>
        <%= link_to "thierry.jet@kombicode.com", new_contact_path("navbar"), class: "navbar-text inline-block mt-0 mr-4 mb-0 lin", data: { action: "click->extended-modal#open", 'turbo-frame': 'contact_modal' } %>
      </div>
    </div>
  </nav>
</header>
